{
  "name": "swiss-element",
  "private": true,
  "version": "0.0.0",
  "description": "Functional custom element library",
  "workspaces": {
    "packages": [
      "packages/*",
      "fixtures/*"
    ],
    "nohoist": [
      "**"
    ]
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint packages/*/src",
    "format": "prettier --write \"{packages,fixtures}/*/{src,test}/**/*.js\" \"**/*.md\"",
    "test": "karma start",
    "coverage": "codecov",
    "build": "rollup -c scripts/rollup/config.js",
    "watch": "NODE_ENV=dev rollup -wc scripts/rollup/config.js",
    "docs": "node scripts/docs.js && npm run format:docs",
    "format:docs": "prettier --write \"**/*.md\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luwes/swiss-element.git"
  },
  "keywords": [
    "functional",
    "custom",
    "element"
  ],
  "author": {
    "name": "Wesley Luyten",
    "email": "me@wesleyluyten.com",
    "url": "wesleyluyten.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/swiss-element/issues"
  },
  "homepage": "https://github.com/luwes/swiss-element#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "@webcomponents/custom-elements": "^1.2.1",
    "ansi-colors": "^3.2.3",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-import": "^2.15.0",
    "fs-extra": "^7.0.1",
    "jsdoc-to-markdown": "^4.0.1",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.0-rc.2",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "prettier": "^1.16.1",
    "puppeteer": "^1.11.0",
    "ramda": "^0.26.1",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0"
  }
}
