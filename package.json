{
  "name": "swiss-element",
  "private": true,
  "version": "0.0.0",
  "description": "Functional custom element library",
  "workspaces": {
    "packages": [
      "packages/*",
      "fixtures/*"
    ],
    "nohoist": [
      "**"
    ]
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint packages/*/src",
    "format": "prettier --write \"packages/*/{src,test}/**/*.js\" \"**/*.md\"",
    "test": "karma start --single-run",
    "coverage": "codecov",
    "build": "rollup -c scripts/rollup/config.js",
    "watch": "rollup -wc scripts/rollup/config.js"
  },
  "esm": {
    "mainFields": [
      "module",
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luwes/swiss-element.git"
  },
  "keywords": [
    "functional",
    "custom",
    "element"
  ],
  "author": {
    "name": "Wesley Luyten",
    "email": "me@wesleyluyten.com",
    "url": "wesleyluyten.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/swiss-element/issues"
  },
  "homepage": "https://github.com/luwes/swiss-element#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@webcomponents/custom-elements": "^1.2.1",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-import": "^2.14.0",
    "karma": "^3.1.4",
    "karma-babel-preprocessor": "^8.0.0-beta.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.0-rc.2",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "mutationobserver-shim": "^0.3.3",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "puppeteer": "^1.11.0",
    "ramda": "^0.26.1",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0"
  }
}
